<li class="term-table">
  <h2>
    <span>{{label}}</span>
    {{#if unitsLong}}
      <span> in {{unitsLong}}</span>
    {{/if}}
  </h2>
  <article>
    {{#each definition as |paragraph|}}
      <p>{{paragraph}}</p>
    {{/each}}
    <table>
      {{#each table as |row|}}
        <tr>
          {{#each row as |cell|}}
            <td>{{ cell }}</td>
          {{/each}}
        </tr>
      {{/each}}
    </table>
  </article>
</li>
